#include <stdio.h>
int main()
{
    int n, sum;
    printf("Enter A integer number for make table\n");
    scanf("%d", &n);
    printf("****Multiplication table of %d****\n\n", n);
    for (int i = 10; i; i--)
    {
        sum += n*i;
        printf("%d X %d = %d\n", n, i, n * i);

    }
     printf("sum of table %d is = %d", n, sum);
    return 0;
}
